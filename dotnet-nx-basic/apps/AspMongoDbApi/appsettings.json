{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:5000"
      },
      "HttpsInlineCertFile": {
        "Url": "https://+:5001",
        "Certificate": {
          "Path": "./aspnetapp.pfx",
          "Password": "SecurePwdGoesHere"
        }
      }
    }
  },
  "BookStoreDatabase": {
    "ConnectionString": "mongodb://root:password@mongodb:27017",
    "DatabaseName": "BookStore",
    "BooksCollectionName": "Books"
  }
}
